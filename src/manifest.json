{
  "manifest_version": 2,
  "name": "Dreamwidth Tools",
  "version": "4.0.2",
  "description": "Unofficial Dreamwidth Tools",
  "applications": {
    "gecko": {
      "id": "dwtools@ashleywr.com"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {

  },
  "web_accessible_resources": [
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.dreamwidth.org/*"
      ],
      "css": [
        "dwtools/css/iconselect.css"
      ],
      "js": [
        "dwtools/js/jquery.js",
        "dwtools/js/spin.js",
        "dwtools/js/iconselect.max.js",
        "dwtools/js/rangyinputs_jquery.min.js",
        "code_firefox.js"

      ]
    }
  ]
}